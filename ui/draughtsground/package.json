{
  "name": "draughtsground",
  "version": "7.6.0",
  "description": "lidraughts.org Draughts UI",
  "type": "commonjs",
  "main": "draughtsground.js",
  "types": "draughtsground.d.ts",
  "directories": {},
  "dependencies": {},
  "devDependencies": {
    "@build/rollupProject": "2.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "@rollup/plugin-node-resolve": "^8",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "draughts": "2.0.0",
    "eslint": "^7",
    "prettier": "^2",
    "rollup": "^2",
    "rollup-plugin-terser": "^7",
    "tslib": "^2",
    "typescript": "^4",
    "fancy-log": "^1",
    "tsify": "^4"
  },
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc --outDir . --sourceMap --declaration",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dist": "rollup --config",
    "dev": "rollup --config",
    "prod": "rollup --config --config-prod",
    "plugin-dev": "rollup --config --config-plugin",
    "plugin-prod": "rollup --config --config-prod --config-plugin"
  },
  "files": [
    "/*.js",
    "/*.d.ts",
    "/*.js.map",
    "/assets/*.css",
    "/src/*.ts",
    "!/rollup.config.js"
  ],
  "keywords": [
    "draughts",
    "lidraughts.org",
    "draughtsboard",
    "UI",
    "typescript"
  ],
  "author": "Stefan Schermann (modified original by Thibault Duplessis)",
  "license": "GPL-3.0"
}
